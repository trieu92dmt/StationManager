@model PrintMoldModel
@{
    ViewBag.Title = LanguageResource.MasterData_PrintMold;
    string CurrentController = ConstController.PrintMold;
    string CurrentArea = ConstArea.MasterData;

    string CurrentUrl = HtmlExtensions.GetCurrentUrl(CurrentArea, CurrentController);
}
<div class="content-header clearfix">
    <h1 class="pull-left">
        @ViewBag.Title
    </h1>
    <div class="pull-right">
        @HtmlExtensions.CreateButton(CurrentArea, CurrentController)
        @*@HtmlExtensions.ExportButton(CurrentArea, CurrentController)
        @HtmlExtensions.ImportButton(CurrentArea, CurrentController)*@
    </div>
</div>
<div class="content">
    <div class="form-horizontal">
        <div class="panel-group">
            <div class="panel panel-default panel-search">
                @using (Html.BeginForm("_Search", null, FormMethod.Post, new { @id = "frmSearch", @class = "isd-form-search" }))
                {
                    <div class="panel-body">
                        <div class="row">

                            <div class="col-md-5">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        @Html.TooltipLabelFor(p => p.PrintMoldCode)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.TextBoxFor(p => p.PrintMoldCode, new { @class = "form-control text-box single-line" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-5">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        @Html.TooltipLabelFor(p => p.PrintMoldName)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.TextBoxFor(p => p.PrintMoldName, new { @class = "form-control text-box single-line" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-5">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        @Html.TooltipLabelFor(p => p.PrintMoldType)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.TextBoxFor(p => p.PrintMoldType, new { @class = "form-control text-box single-line" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-5">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        @Html.TooltipLabelFor(p => p.Serial)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.TextBoxFor(p => p.Serial, new { @class = "form-control text-box single-line" })
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-5">
                                <div class="form-group">
                                    <div class="col-md-4">
                                        @Html.TooltipLabelFor(p => p.Bin)
                                    </div>
                                    <div class="col-md-8">
                                        @Html.TextBoxFor(p => p.Bin, new { @class = "form-control text-box single-line" })
                                    </div>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-md-5">
                                    <div class="form-group">
                                        <div class="col-md-4">
                                            @Html.TooltipLabelFor(p => p.ProductId)
                                        </div>
                                        <div class="col-md-8">
                                            @Html.DropDownListFor(p => p.ProductId, null, LanguageResource.Dropdownlist_All, new { @class = " form-control with-search" })
                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-5">
                                    <div class="form-group">
                                        <div class="col-md-4">
                                            @Html.TooltipLabelFor(p => p.ProfileName)
                                        </div>
                                        <div class="col-md-8">
                                            @Html.TextBoxFor(p => p.ProfileName, new { @class = "form-control text-box single-line" })
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                        <div class="col-md-12">
                            <div class="form-group">
                                <div class="col-md-8 col-md-offset-5">
                                    @HtmlExtensions.SearchButton()
                                </div>
                            </div>
                        </div>
                    </div>
                }
            </div>

            <div class="panel panel-default">
                <div class="panel-body">
                    <div id="divSearchResult">

                        @*<table id="table-view" class="table table-dark table-bordered dataTableServerSide">
                            <thead class="isd-table-header">
                                <tr>
                                    <th class="text-center isd-table-th-stt">@LanguageResource.NumberIndex</th>
                                    <th class="text-center">@Html.DisplayNameFor(p => p.PrintMoldIntId)</th>
                                    <th>@Html.DisplayNameFor(p => p.PrintMoldType)</th>
                                    <th>@Html.DisplayNameFor(p => p.PrintMoldCode)</th>
                                    <th>@Html.DisplayNameFor(p => p.PrintMoldName)</th>
                                    <th>@Html.DisplayNameFor(p => p.ProfileName)</th>
                                    <th>@Html.DisplayNameFor(p => p.ProductId)</th>
                                    <th>@Html.DisplayNameFor(p => p.Specifications_Length)</th>
                                    <th>@Html.DisplayNameFor(p => p.Specifications_Width)</th>
                                    <th>@Html.DisplayNameFor(p => p.Specifications_Height)</th>
                                    <th>@Html.DisplayNameFor(p => p.Specifications_Overalls)</th>
                                    <th>@Html.DisplayNameFor(p => p.Specifications_Side)</th>
                                    <th>@Html.DisplayNameFor(p => p.ProductPerMold)</th>
                                    <th>@Html.DisplayNameFor(p => p.PrintMoldFilm)</th>
                                    <th>@Html.DisplayNameFor(p => p.PrintMoldDate)</th>
                                    <th>@Html.DisplayNameFor(p => p.Bin)</th>
                                    <th>@Html.DisplayNameFor(p => p.LocationNote)</th>
                                    <th>@Html.DisplayNameFor(p => p.Status)</th>
                                    <th>@Html.DisplayNameFor(p => p.LastMaintenanceDate)</th>
                                    <th>@Html.DisplayNameFor(p => p.MaintenanceAlert)</th>
                                    <th>@Html.DisplayNameFor(p => p.StampQuantity)</th>
                                    <th>@Html.DisplayNameFor(p => p.CurrentStampeQuantity)</th>
                                    <th>@Html.DisplayNameFor(p => p.StampQuantityAlert)</th>
                                    <th>@Html.DisplayNameFor(p => p.Description)</th>
                                    <th class="text-center">@LanguageResource.Actions</th>
                                </tr>
                            </thead>
                            <tbody class="tbody">
                            </tbody>
                        </table>*@
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="delete-confirm"></div>
@section scripts {
    <script>
    //load default and set event
    $(document).ready(function () {
        ISD.SearchInitial("@CurrentUrl", true);
        ISD.ShowMessage(window.location.href);

    });
   
    </script>
}

