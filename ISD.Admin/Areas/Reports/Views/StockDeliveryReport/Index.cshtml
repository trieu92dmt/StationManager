                else {
                    if ($("#SearchProductId" + i).val() != undefined) {
                        listProductId.push($("#SearchProductId" + i).val());
                    }

                }
            }
            var obj = {};
            $.each(data, function (index, val) {
                obj[val.name] = val.value;
                $.extend(true, arr, obj)
            });
            obj["ListSearchProductId"] = listProductId;
            obj["TemplateId"] = $('#TemplateId').val();
            $.extend(true, arr, obj);
