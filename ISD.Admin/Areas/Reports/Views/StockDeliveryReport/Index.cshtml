                    if (ui.item === null) {
                        $("#ProductCode" + index).val("");
                        $("#ProductName" + index).val("");
                        $("#SearchProductId" + index).val("");
                        $("#ProductCode" + index).focus();
                    }
                },
                select: function (event, ui) {
                    $("#ProductCode" + index).val(ui.item.value);
                    $("#ProductCode" + index).prop("disabled", true);
                    $("#ProductName" + index).val(ui.item.name);
                    $("#SearchProductId" + index).val(ui.item.id);
                }
            });
        });


